! Keywords for identification of orbits
! Keywords per il propagatore
INPUT: propag.key
! Keywords specifiche di ident/ident3/identmulti
ident.
        .d2max           REA     ! d2 control MAX
        .d5max           REA     ! d5 control MAX
        .d4max           REA     ! d4 control MAX
        .d6max           REA     ! d6 control MAX
        .d6amax          REA     ! d6a control MAX
        .d6amin          REA     ! d6a contol MIN  
        .d2min           REA     ! d2 control MIN
        .d4min           REA     ! d4 control MIN
        .d5min           REA     ! d5 control MIN
        .d6min           REA     ! d6 control MIN 
        .distmax         REA     ! distance control MAX
        .distmin         REA     ! distance control MIN
        .dmagn           REA     ! magnitude difference control MAX
        .nmax            INT     ! max number orbits to be read
        .use_enne        LOG    ! use mean motion as variable
        .coox            CHA    ! coordinates for difcor (CAR EQU KEP COM ATT)
        .catalog1        CHA    ! input orbit catalog 1
        .catalog2        CHA    ! input orbit catalog 2
        .catalog3        CHA    ! input orbit catalog 3
        .catalog4        CHA    ! input orbit catalog 4
        .catalog5        CHA    ! input orbit catalog 5
        .catalog6        CHA    ! input orbit catalog 6
!
