! keywords for resret2.f

INPUT: time.key
INPUT: propag.key
INPUT: difcor.key
INPUT: obserr.key
INPUT: iers.key

resret.
        .dt             REA    ! shower length (days)
        .tgap           REA    ! shower gap (days)
        .dnewton        REA    ! control for startup of newton
! where to find input/ to write output
        .clodir         CHA    ! directory for close approach records (input)
        .repdir         CHA    ! directory for report files (output)
        .mulsodir       CHA    ! directory containing multiple solutions
        .obsdir         CHA    ! directory containing observation data
! data from neomul.nop/cateph.mop
        .smax           REA    ! span on sigma space
        .deltasig       REA    ! delta sigma (between two multiple solutions)
        .nmul           INT    ! number of multiple solutions
        .dmin_used      REA    ! radius of target plane used by cateph2
!
