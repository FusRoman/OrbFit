! options file for attribute/attcheck
! version to be used for attribute

vsatri.
.obsdir      CHA    ! where to find/put observations
.ephdir      CHA    ! where to write ephemerides (vsa_ephem/vsa_ident)
.tridir      CHA    ! O triangulations (vsa_triang) I (vsa_ephem,vsa_ident)
.rwoiddir    CHA    ! directory for .rwo files of identif (vsa_ident)
.npo         INT    ! number of points for triangulation (vsa_triang)
.pmult       INT    ! multiplier of the same (vsa_triang)
.dt          REA    ! time between attributable and prediction (vsa_ephem)
.h_max       REA    ! absolute magnitude limit (default 30) (vsa_triang)
.distmax     REA    ! control on identification metric (vsa_ident)
INPUT: propag.key
INPUT: time.key

